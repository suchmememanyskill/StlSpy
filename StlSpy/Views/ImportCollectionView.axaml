<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="StlSpy.Views.ImportCollectionView">
    <StackPanel HorizontalAlignment="Center" Spacing="10" Margin="20">
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top" Spacing="10">
            <TextBox Watermark="Enter Share ID Here" Name="ShareId" Width="400" />
            <Button Name="Submit" Content="Import Share" IsDefault="True" />
        </StackPanel>
        <Label Foreground="Red" Name="ErrorText" HorizontalAlignment="Center" />
    </StackPanel>
</UserControl>
