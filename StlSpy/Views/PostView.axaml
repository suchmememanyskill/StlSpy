<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="StlSpy.Views.PostView">
    <Border Margin="5" CornerRadius="5" Background="#202020" Width="800" Padding="5">
        <Panel>
            <StackPanel>
                <Button Name="PostName" Classes="invisible url" Padding="5,5,5,0" FontSize="20" />
                <Button Name="PostAuthor" Classes="invisible url" Padding="5,0" FontSize="16" />
            </StackPanel>
        </Panel>
    </Border>
</UserControl>
