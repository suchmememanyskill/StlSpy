<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="StlSpy.Views.PreviewPostView">
    <DockPanel Width="300" Height="250" Name="Panel" Background="Red">
        <StackPanel DockPanel.Dock="Top" Height="40" VerticalAlignment="Center" >
            <Label FontSize="18" VerticalContentAlignment="Center" HorizontalAlignment="Center" Height="40" Name="Title">Hello</Label>
        </StackPanel>
        <Image Width="300" Height="210" Name="Background" Stretch="UniformToFill"  />
    </DockPanel>
</UserControl>
