<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="StlSpy.MainWindow"
        Title="StlSpy">
    <DockPanel>
        <Panel Background="OrangeRed" Height="50" DockPanel.Dock="Top" Name="HeaderBackground">
            <StackPanel Orientation="Horizontal" VerticalAlignment="Center" Margin="10,0">
                <Label Name="MainText" FontWeight="Bold" FontSize="20" Content="StlSpy" />
                <Label Name="SubText" FontSize="20" />
            </StackPanel>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Spacing="10" Name="StackPanel" />
        </Panel>
        <Panel Name="MainContent" />
    </DockPanel>
</Window>
